{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order": {
      "type": "component",
      "repeatable": true,
      "component": "entities.order",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "fullname": {
      "type": "string",
      "required": true
    },
    "totalPrice": {
      "type": "integer",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Processing",
        "Delivered",
        "Cancelled"
      ],
      "default": "Processing",
      "required": false
    },
    "uuid": {
      "type": "uid"
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "remarks": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "street": {
      "type": "string"
    }
  }
}
